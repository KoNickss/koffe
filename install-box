pacman-key --populate archlinux
pacman-key --refresh-keys
pacman -Sy --noconfirm wget pacman-contrib libnewt archiso nano gzip
rm ~/.bashrc
wget https://raw.githubusercontent.com/KoNickss/koffe/main/.bashrc -P ~/
rm /usr/bin/pkgdown
