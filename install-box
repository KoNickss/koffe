chmod +x /usr/bin/pkgdown
rm /usr/bin/register2
wget https://raw.githubusercontent.com/KoNickss/koffe/main/register2 -P /usr/bin/
chmod +x /usr/bin/register2
mkdir /arch
mkdir /iso
mkdir/arch/temp
mkdir /arch/scripts
mkdir /arch/pkgs
